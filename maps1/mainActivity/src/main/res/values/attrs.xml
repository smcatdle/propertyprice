<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XYPlot">
        <attr name="androidplot.title" format="string"/>
    </declare-styleable>
    
    <declare-styleable name="Plot">
        <attr name="plotLabel" format="string"/>
        <attr name="plotLabelTextSize" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="xy.XYPlot" parent="@style/Plot">
        <attr name="domainLabel" format="string"/>
        <attr name="domainLabelTextSize" format="dimension"/>
        <attr name="rangeLabel" format="string"/>
        <attr name="rangeLabelTextSize" format="dimension"/>
        <attr name="graphMarginTop" format="dimension"/>
        <attr name="graphMarginBottom" format="dimension"/>
        <attr name="graphMarginLeft" format="dimension"/>
        <attr name="graphMarginRight" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="MiniGraph">
   </declare-styleable>
</resources>